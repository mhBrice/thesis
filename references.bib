@article{saura_key_2011,
  title = {Key Structural Forest Connectors Can Be Identified by Combining Landscape Spatial Pattern and Network Analyses},
  author = {Saura, Santiago and Vogt, Peter and Vel{\'a}zquez, Javier and Hernando, Ana and Tejera, Rosario},
  year = {2011},
  month = jul,
  volume = {262},
