@article{canham_neighborhood_2006,
  title = {Neighborhood {{Analyses Of Canopy Tree Competition Along Environmental Gradients In New England Forests}}},
  author = {Canham, Charles D. and Papaik, Michael J. and Uriarte, Mar{\'i}a and McWilliams, William H. and Jenkins, Jennifer C. and Twery, Mark J.},
  year = {2006},
  month = apr,
  volume = {16},
  pages = {540--554},
