@article{neilson_transient_1993,
  title = {Transient Ecotone Response to Climatic Change: {{Some}} Conceptual and Modelling Approaches},
  shorttitle = {Transient Ecotone Response to Climatic Change},
  author = {Neilson, Ronald P.},
