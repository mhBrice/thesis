  file = {/Users/mariehbrice/Zotero/storage/IFBMCB3A/Landhäusser et al_2010_Disturbance facilitates rapid range expansion of aspen into higher elevations.pdf},
  journal = {Journal of Biogeography},
  keywords = {Boreal,Canadian Rocky Mountains,climate warming,disturbance,invasion,microtopography,montane,Populus tremuloides,seedbed,seedling establishment},
  language = {en},
  number = {1}
}

@article{lankau_mycorrhizal_nodate,
