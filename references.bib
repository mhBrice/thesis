@article{danneyrolles_anthropogenic_2018,
  title = {Anthropogenic Disturbances Strengthened Tree Community-Environment Relationships at the Temperate-Boreal Interface},
  author = {Danneyrolles, Victor and Arseneault, Dominique and Bergeron, Yves},
