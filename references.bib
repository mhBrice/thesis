@article{madrigal-gonzalez_disentangling_2018,
  title = {Disentangling the Relative Role of Climate Change on Tree Growth in an Extreme {{Mediterranean}} Environment},
  author = {{Madrigal-Gonz{\'a}lez}, Jaime and Andivia, Enrique and Zavala, Miguel A. and Stoffel, Markus and Calatayud, Joaqu{\'i}n and {S{\'a}nchez-Salguero}, Ra{\'u}l and {Ballesteros-C{\'a}novas}, Juan},
