  author = {Thom, Dominik and Rammer, Werner and Dirnb{\"o}ck, Thomas and M{\"u}ller, J{\"o}rg and Kobler, Johannes and Katzensteiner, Klaus and Helm, Norbert and Seidl, Rupert},
  year = {2017},
