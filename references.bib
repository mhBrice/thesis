  copyright = {\textcopyright{} 2016 John Wiley \& Sons Ltd},
  file = {/Users/mariehbrice/Zotero/storage/4U3J7YYZ/Searle and Chen - 2017 - Persistent and pervasive compositional shifts of w.pdf},
