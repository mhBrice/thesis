@article{brown_geographic_1996,
  title = {{{THE GEOGRAPHIC RANGE}}: {{Size}}, {{Shape}}, {{Boundaries}}, and {{Internal Structure}}},
  shorttitle = {{{THE GEOGRAPHIC RANGE}}},
  author = {Brown, James H. and Stevens, George C. and Kaufman, Dawn M.},
