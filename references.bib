@article{dalsgaard_specialization_2011,
  title = {Specialization in {{Plant}}-{{Hummingbird Networks Is Associated}} with {{Species Richness}}, {{Contemporary Precipitation}} and {{Quaternary Climate}}-{{Change Velocity}}},
  author = {Dalsgaard, Bo and Mag{\aa}rd, Else and Fjelds{\aa}, Jon and Mart{\'i}n Gonz{\'a}lez, Ana M. and Rahbek, Carsten and Olesen, Jens M. and Ollerton, Jeff and Alarc{\'o}n, Ruben and Cardoso Araujo, Andrea and Cotton, Peter A. and Lara, Carlos and Machado, Caio Graco and Sazima, Ivan and Sazima, Marlies and Timmermann, Allan and Watts, Stella and Sandel, Brody and Sutherland, William J. and Svenning, Jens-Christian},
