  author = {Girardin, Martin P. and Ali, Adam A. and Carcaillet, Christopher and Gauthier, Sylvie and H{\'e}ly, Christelle and Le Goff, H{\'e}lo{\"i}se and Terrier, Aur{\'e}lie and Bergeron, Yves},
