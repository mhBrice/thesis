@article{gravel_patch_2010,
  title = {Patch {{Dynamics}}, {{Persistence}}, and {{Species Coexistence}} in {{Metaecosystems}}},
  author = {Gravel, Dominique and Mouquet, Nicolas and Loreau, Michel and Guichard, Fr{\'e}d{\'e}ric},
  year = {2010},
  month = sep,
  volume = {176},
  pages = {289--302},
  issn = {0003-0147, 1537-5323},
  doi = {10.1086/655426},
  abstract = {We add an ecosystem perspective to spatially structured communities subject to colonization-extinction dynamics. We derive a plant-based metaecosystem model to analyze how the spatial flows of the biotic and abiotic forms of a limiting nutrient affect persistence and coexistence. We show that the proportion of patches supporting plants in a region has a considerable impact on local nutrient dynamics. Then we explicitly couple nutrient dynamics to patch dynamics. Our model shows strong feedback between local and regional dynamics mediated by nutrient flows. We find that nutrient flows can have either positive or negative effects on species persistence and coexistence. The essential feature of this local-regional coupling is the net direction of the nutrient flows between occupied and empty patches. A net flow of nutrients from occupied to empty patches leads to indirect facilitative interactions, such as an inferior competitor promoting the persistence of a superior competitor. We show that nutrient flows affect the potential diversity of the metaecosystem and key features of plant community dynamics, such as the shape of the competition-colonization trade-off and successional sequences. Our analysis revealed that integrating ecosystem and spatial dynamics can lead to various indirect interactions that contribute significantly to community organization.},
  file = {/Users/mariehbrice/Zotero/storage/JXS5ECBS/Gravel et al. - 2010 - Patch Dynamics, Persistence, and Species Coexisten.pdf},
  journal = {The American Naturalist},
  language = {en},
  number = {3}
}

@article{gravel_reconciling_2006,
  title = {Reconciling Niche and Neutrality: The Continuum Hypothesis: {{Reconciling}} Niche and Neutrality},
  shorttitle = {Reconciling Niche and Neutrality},
  author = {Gravel, Dominique and Canham, Charles D. and Beaudet, Marilou and Messier, Christian},
