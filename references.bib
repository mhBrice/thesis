}

@article{woodall_indicator_2009,
  title = {An Indicator of Tree Migration in Forests of the Eastern {{United States}}},
  author = {Woodall, C.W. and Oswalt, C.M. and Westfall, J.A. and Perry, C.H. and Nelson, M.D. and Finley, A.O.},
