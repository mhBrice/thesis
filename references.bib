@article{gomez-aparicio_disentangling_2011,
  title = {Disentangling the Relative Importance of Climate, Size and Competition on Tree Growth in {{Iberian}} Forests: Implications for Forest Management under Global Change},
  shorttitle = {Disentangling the Relative Importance of Climate, Size and Competition on Tree Growth in {{Iberian}} Forests},
  author = {{G{\'o}mez-Aparicio}, Lorena and {Garc{\'i}a-Vald{\'e}s}, Ra{\'u}L and {Ru{\'i}z-Benito}, Paloma and Zavala, Miguel A.},
